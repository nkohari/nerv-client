{
  "name": "mineboss-client",
  "version": "0.2.0",
  "description": "management UI for mineboss",
  "engines": {
    "node": "^6.9.0",
    "npm": "5"
  },
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production node -r ts-node/register ./node_modules/.bin/webpack --progress --config ./build/webpack.config.production.ts",
    "dev": "NODE_ENV=development node -r ts-node/register ./node_modules/.bin/webpack-dev-server --hot --history-api-fallback --port 8081 --config ./build/webpack.config.development.ts",
    "clean": "rimraf dist",
    "deploy": "npm run build && s3-deploy './dist/**' --cwd './dist/' --gzip --region us-east-1 --bucket retrium-customer-success.enkari.com"
  },
  "author": "Nate Kohari",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "1.19.0",
    "classnames": "^2.2.5",
    "copy-to-clipboard": "^3.0.6",
    "joi": "^10.6.0",
    "moment": "^2.18.1",
    "nconf": "^0.8.4",
    "nes": "^6.4.2",
    "react": "^15.3.2",
    "react-addons-css-transition-group": "^15.6.0",
    "react-dom": "^15.3.2",
    "react-recaptcha": "^2.3.2",
    "react-redux": "^5.0.5",
    "react-sparklines": "^1.6.0",
    "react-transition-group": "^1.1.2",
    "react-virtualized": "^9.8.0",
    "redux": "^3.7.1",
    "redux-actions": "^2.2.0",
    "redux-little-router": "^14.2.0",
    "redux-localstorage": "^0.4.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "ts-node": "^3.1.0",
    "typescript": "2.3.3",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@types/joi": "^10.4.0",
    "@types/node": "^6.0.79",
    "@types/react": "^15.0.33",
    "@types/react-dom": "^15.5.1",
    "@types/react-virtualized": "^9.7.2",
    "@types/redux-actions": "^1.2.6",
    "@types/webpack": "^3.0.1",
    "@types/webpack-env": "^1.13.0",
    "css-loader": "^0.28.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "raw-loader": "^0.5.1",
    "react-hot-loader": "^1.3.1",
    "s3-deploy": "^0.7.5",
    "style-loader": "0.17.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "svg-react-loader": "^0.4.4",
    "ts-loader": "^2.2.1",
    "tslint": "^5.4.3",
    "tslint-microsoft-contrib": "^5.0.0",
    "tsutils": "1.6.0",
    "url-loader": "^0.5.7",
    "webpack": "2.6.1",
    "webpack-dev-server": "^2.5.0"
  }
}
